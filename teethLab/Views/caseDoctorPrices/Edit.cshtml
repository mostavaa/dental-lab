@model teethLab.caseDoctorPrice

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>caseDoctorPrice</legend>

        @Html.HiddenFor(model => model.id)

        <div class="editor-label">
            @Html.LabelFor(model => model.doctorId, "doctor")
        </div>
        <div class="editor-field">
            @Html.DropDownList("doctorId", String.Empty)
            @Html.ValidationMessageFor(model => model.doctorId)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.caseId, "CurrentCas")
        </div>
        <div class="editor-field">
            @Html.DropDownList("caseId", String.Empty)
            @Html.ValidationMessageFor(model => model.caseId)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.price)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.price)
            @Html.ValidationMessageFor(model => model.price)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
